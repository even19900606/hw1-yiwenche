<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>GeneNameNERTypeSystem</name>
  <description>Type System Definition for the Gene Name NER</description>
  <version>1.0</version>
  <vendor>The Apache Software Foundation</vendor>
  <types>
    <typeDescription>
      <name>model.GeneName</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>ID</name>
          <description>The Sentence ID that contain this gene name</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>Name</name>
          <description>Gene's name</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>Accumulate</name>
          <description>Accumulate offset generated by previous sentences</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
